{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development"
    },
    "preview": {
      "distribution": "internal",
      "environment": "preview"
    },
    "production": {
      "autoIncrement": true,
      "environment": "production"
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true,
        "buildNumber": "2"
      },
      "environment": "development"
    }
  },
  "submit": {
    "production": {}
  }
}
